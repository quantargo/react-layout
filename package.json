{
  "name": "@quantargo/react-layout",
  "version": "1.1.1",
  "description": "Drop-in layout components with zero configuration",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "storybook": "start-storybook -p 6006",
